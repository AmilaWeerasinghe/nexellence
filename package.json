{
  "name": "nexellence",
  "version": "1.0.1",
  "description": "Web Application for an institute management",
  "author": "luk3Sky",
  "license": "ISC",
  "engines": {
    "node": "4.2.1"
  },
  "main": "server.js",
  "scripts": {
    "webpack": "webpack",
    "webpack:dev": "webpack --watch",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "method-override": "^2.3.5",
    "moment": "^2.11.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "history": "^1.17.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2"
  }
}
