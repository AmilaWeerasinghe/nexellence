{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    },
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "plugins": [
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "rules": {
    "import/first": 2,
    "no-console": "warn",
    "no-eval": "error",
    "no-unused-vars": "warn",
    "no-plusplus": "error",
    "no-duplicate-imports": "error",
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "react/jsx-props-no-spreading": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/label-has-associated-control": [
      "error", 
      {
      "required": {
        "some": [ "nesting", "id"  ]
        }
      }
    ],
    "semi": 1
  }
}